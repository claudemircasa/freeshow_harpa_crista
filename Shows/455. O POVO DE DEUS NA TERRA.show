["bc3813597fcd",{"name":"455. O POVO DE DEUS NA TERRA","private":false,"category":"6d75606a954","settings":{"activeLayout":"61e20bc3813","template":"default"},"timestamps":{"created":1662168845534,"modified":null,"used":null},"meta":{},"slides":{"1e20bc38135":{"group":"","color":null,"globalGroup":"","settings":{},"notes":"","items":[{"style":"left:50px;top:120px;width:1820px;height:840px;","lines":[{"align":"","text":[{"style":"font-size: 80px;","value":""}]},{"align":"","text":[{"style":"font-size: 80px;","value":"1. O povo de Deus, aqui na terra tem um sinal:"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Povo que vive em santa guerra contra o mal"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Povo que espera a Jesus Cristo, que presto vem"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"É pois Ele mesmo, quem nos diz isto: \"Vigiai mui bem\""}]},{"align":"","text":[{"style":"font-size: 80px;","value":""}]}]}]},"e20bc381359":{"group":"verse","color":null,"globalGroup":"verse","settings":{},"notes":"","items":[{"style":"left:50px;top:120px;width:1820px;height:840px;","lines":[{"align":"","text":[{"style":"font-size: 80px;","value":"2. A minha paz, sim, não a do mundo, Eu vos darei"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"O coração de gozo profundo, Eu encherei"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Com o Seu braço Ele nos guia, oh! grande amor"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Para os que Lhe pedem, logo envia o Consolador!"}]},{"align":"","text":[{"style":"font-size: 80px;","value":""}]}]}]},"20bc3813597":{"group":"verse","color":null,"globalGroup":"verse","settings":{},"notes":"","items":[{"style":"left:50px;top:120px;width:1820px;height:840px;","lines":[{"align":"","text":[{"style":"font-size: 80px;","value":"3. Ó vinde a Mim, Jesus convida, quem sede tem!"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Beber, ó vinde, da água da vida, celeste bem!"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Cristo sacia a alma sedenta do pecador!"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Salva, purifica e alimenta, enche de amor!"}]},{"align":"","text":[{"style":"font-size: 80px;","value":""}]}]}]},"0bc3813597f":{"group":"verse","color":null,"globalGroup":"verse","settings":{},"notes":"","items":[{"style":"left:50px;top:120px;width:1820px;height:840px;","lines":[{"align":"","text":[{"style":"font-size: 80px;","value":"4.Em Cristo crendo sempre O seguindo quão doce é"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"A paz profunda e o amor sentindo: Tudo por fé"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Feliz o homem que guarda isto, pois vida tem"}]},{"align":"","text":[{"style":"font-size: 80px;","value":"Há de reinar sempre com Jesus Cristo na mansão de além"}]},{"align":"","text":[{"style":"font-size: 80px;","value":""}]}]}]}},"layouts":{"61e20bc3813":{"name":"Padrão","notes":"","slides":[{"id":"1e20bc38135"},{"id":"e20bc381359"},{"id":"20bc3813597"},{"id":"0bc3813597f"}]}},"media":{}}]